{
   "name": "type-orm",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./ormconfig.ts",
      "migration-generate": "npm run typeorm:cli -- migration:generate -n migration -d src/migrations",
      "migration-run": "npm run typeorm:cli -- migration:run ",
      "migration-create": "npm run typeorm:cli -- migration:create -n migration -d src/migrations",
      "pretty": "prettier --write \"./**/*.{ts}\""
   },
   "dependencies": {
      "@apollo/gateway": "^0.42.3",
      "@nestjs/common": "^8.0.0",
      "@nestjs/config": "^1.1.6",
      "@nestjs/core": "^8.0.0",
      "@nestjs/graphql": "^9.1.2",
      "@nestjs/microservices": "^8.2.6",
      "@nestjs/platform-express": "^8.0.0",
      "@nestjs/platform-socket.io": "^8.2.6",
      "@nestjs/typeorm": "^8.0.2",
      "@nestjs/websockets": "^8.2.6",
      "apollo-server-express": "^3.6.1",
      "class-transformer": "^0.4.0",
      "class-validator": "^0.11.1",
      "graphql": "^15.8.0",
      "graphql-redis-subscriptions": "^2.4.2",
      "ioredis": "^4.28.3",
      "mysql": "^2.14.1",
      "node": "^12.22.8",
      "pg": "^8.7.1",
      "reflect-metadata": "^0.1.10",
      "rimraf": "^3.0.2",
      "rxjs": "^7.5.2",
      "socket.io": "^4.4.1",
      "ts-morph": "^13.0.2",
      "typeorm": "0.2.41"
   },
   "devDependencies": {
      "@nestjs/cli": "^8.1.8",
      "@nestjs/schematics": "^8.0.0",
      "@nestjs/testing": "^8.0.0",
      "@types/express": "^4.17.13",

      "@types/jest": "27.0.2",
      "@types/node": "^17.0.8",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^5.9.1",
      "@typescript-eslint/parser": "^5.9.1",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^4.0.0",
      "jest": "^27.2.5",
      "prettier": "^2.3.2",
      "source-map-support": "^0.5.20",
      "supertest": "^6.2.0",
      "ts-jest": "^27.0.3",
      "ts-loader": "^9.2.3",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.10.1",
      "typescript": "^4.5.4"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
