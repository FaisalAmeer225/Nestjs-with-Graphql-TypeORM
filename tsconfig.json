{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "esnext.asynciterable"
      ],
      "target": "ES2017",
      // "target": "es5",
      "module": "commonjs",
      "esModuleInterop": true,
      "baseUrl": "./",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "skipLibCheck": true
   }
}